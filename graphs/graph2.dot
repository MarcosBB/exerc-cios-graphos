graph Graph2 {
  // nós (letras)
  a; b; c; d; e; f; g; h; i; j;

  // ciclo 1 (a-b-c-d-e-a)
  a -- b;
  b -- c;
  c -- d;
  d -- e;
  e -- a;

  // ciclo 2 (f-g-h-i-j-f)
  f -- g;
  g -- h;
  h -- i;
  i -- j;
  j -- f;

  // arestas que conectam os ciclos (garante que não seja uma árvore)
  c -- f;
  b -- h;
}
